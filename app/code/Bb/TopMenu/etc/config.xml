<config>
    <modules>
        <Bb_TopMenu>
            <version>0.1.0</version>
        </Bb_TopMenu>
    </modules>
    <global>
        <blocks>
            <page>
                <rewrite>
                    <html_topmenu>Bb_TopMenu_Block_Html_Topmenu</html_topmenu>
                </rewrite>
            </page>
        </blocks>
        <models>
            <catalog>
                <rewrite>
                    <observer>Bb_TopMenu_Model_Catalog_Observer</observer>
                </rewrite>
            </catalog>
        </models>
        <helpers>
            <bb_topmenu>
                <class>Bb_TopMenu_Helper</class>
            </bb_topmenu>
        </helpers>
    </global>
</config>

